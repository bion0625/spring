<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="hello" class="springbook.learningtest.spring.ioc.xml.autowiring.constructor.bean.Hello" autowire="constructor">
        <constructor-arg name="name" value="autowiringSpringConstructor"/>
    </bean>
    <bean id="mainPrinter" class="springbook.learningtest.spring.ioc.xml.autowiring.constructor.bean.StringPrinter"/>
</beans>