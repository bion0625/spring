<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="hello" class="springbook.learningtest.spring.ioc.xml.property.bean.Hello">
        <property name="printer" ref="defaultPrinter"/>
        <property name="name" value="Spring"/>
        <property name="age" value="30"/>
        <property name="myClass" value="java.lang.String"/>
    </bean>
    <bean id="defaultPrinter" class="springbook.learningtest.spring.ioc.xml.property.bean.StringPrinter"/>
</beans>